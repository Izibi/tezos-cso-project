{
  "name": "tezos-cso-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "npm run db:migrate",
    "dev": "node server.js",
    "build": "next build",
    "serve": "cross-env NODE_ENV=production node server.js",
    "lint": "next lint",
    "db:migrate": "TS_NODE_PROJECT=tsconfig.knex.json knex migrate:latest",
    "db:rollback": "TS_NODE_PROJECT=tsconfig.knex.json knex migrate:rollback"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "next": "11.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "knex": "^0.95.6",
    "pg": "^8.6.0"
  },
  "devDependencies": {
    "@types/react": "17.0.11",
    "autoprefixer": "^10.2.6",
    "eslint": "7.29.0",
    "eslint-config-next": "11.0.0",
    "postcss": "^8.3.5",
    "tailwindcss": "^2.2.2",
    "typescript": "4.3.4",
    "ts-node": "^7.0.1"
  }
}
