{
  "name": "tezos-cso-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "npm run db:migrate",
    "dev": "node server.js",
    "build": "next build",
    "start": "next start -- --port $PORT",
    "serve": "cross-env NODE_ENV=production node server.js",
    "lint": "next lint",
    "_test": "node ./node_modules/puppeteer-cucumber-js/index.js --headless",
    "test": "npm run build && start-server-and-test serve http://localhost:3000 _test",
    "db:migrate": "TS_NODE_PROJECT=tsconfig.knex.json knex migrate:latest",
    "db:rollback": "TS_NODE_PROJECT=tsconfig.knex.json knex migrate:rollback"
  },
  "dependencies": {
    "@fontsource/cormorant": "^4.5.0",
    "@fontsource/libre-franklin": "^4.5.0",
    "@taquito/beacon-wallet": "^9.2.0",
    "@taquito/taquito": "^9.2.0",
    "apexcharts": "^3.27.2",
    "cross-env": "^7.0.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "knex": "^0.95.6",
    "next": "11.0.0",
    "next-auth": "latest",
    "pg": "^8.6.0",
    "react": "17.0.2",
    "react-apexcharts": "^1.3.9",
    "react-dom": "17.0.2",
    "swr": "^0.5.6"
  },
  "devDependencies": {
    "@types/react": "17.0.11",
    "autoprefixer": "^10.2.6",
    "eslint": "7.29.0",
    "eslint-config-next": "11.0.0",
    "postcss": "^8.3.5",
    "puppeteer-cucumber-js": "^1.6.1",
    "start-server-and-test": "^1.12.6",
    "tailwindcss": "^2.2.2",
    "ts-node": "^7.0.1",
    "typescript": "4.3.4"
  }
}
